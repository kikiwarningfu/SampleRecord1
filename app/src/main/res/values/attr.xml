<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RoundImageView">
        <attr name="borderRadius" format="dimension"></attr>
        <attr name="type"/>
        <attr name="square" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="SkeletonBlock">
        <attr name="sb_duration" format="integer"/>
        <!-- Standard orientation constant. -->
        <attr name="sb_orientation">
            <!-- Defines an horizontal widget. -->
            <enum name="horizontal" value="0" />
            <!-- Defines a vertical widget. -->
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>

    <attr name="type">
        <enum name="circle" value="0" />
        <enum name="round" value="1" />
    </attr>


    <declare-styleable name="SwipeMenuLayout">
        <attr name="swipeEnable" format="boolean"/>
        <attr name="ios" format="boolean"/>
        <attr name="leftSwipe" format="boolean"/>
    </declare-styleable>

    <attr name="topbar_background" format="color|reference"/>

    <!--比例布局-->
    <declare-styleable name="PercentRelativeLayout">
        <attr name="widthSize" format="float"/>
        <attr name="heightSize" format="float"/>

        <attr name="fixFlag">
            <enum name="width" value="0"/>
            <enum name="height" value="1"/>
        </attr>
    </declare-styleable>



    <!--搜索导航栏-->
    <declare-styleable name="SearchBar">
        <attr name="textRight" format="string"/>
        <attr name="searchHint" format="string"/>
        <attr name="ivRightHint" format="boolean"/>
        <attr name="textRightHint" format="boolean"/>
        <attr name="ivRightIcon" format="reference"/>
        <attr name="ivLeftIcon" format="reference"/>
    </declare-styleable>



    <!--评分条-->
    <declare-styleable name="RatingBar">
        <attr name="starNormalIcon" format="reference"/>
        <attr name="starCheckedIcon" format="reference"/>
        <attr name="starSize" format="dimension"/>
        <attr name="starPadding" format="dimension"/>
        <attr name="starMaxCount" format="integer"/>
        <attr name="starCurCount" format="integer"/>
    </declare-styleable>



    <!--datePicker-->
    <attr name="halfVisibleItemCount" format="integer"/>
    <attr name="itemTextSize" format="dimension"/>
    <attr name="itemMaximumWidthText" format="string"/>
    <attr name="itemTextColor" format="color"/>
    <attr name="textGradual" format="boolean"/>
    <attr name="selectedTextColor" format="color"/>
    <attr name="selectedTextSize" format="dimension"/>
    <attr name="currentItemPosition" format="integer"/>
    <attr name="itemWidthSpace" format="dimension"/>
    <attr name="itemHeightSpace" format="dimension"/>
    <attr name="zoomInSelectedItem" format="boolean"/>
    <attr name="wheelCyclic" format="boolean"/>
    <attr name="wheelCurtain" format="boolean"/>
    <attr name="wheelCurtainColor" format="color"/>
    <attr name="wheelCurtainBorder" format="boolean"/>
    <attr name="wheelCurtainBorderColor" format="color"/>
    <attr name="indicatorText" format="string"/>
    <attr name="indicatorTextSize" format="dimension"/>
    <attr name="indicatorTextColor" format="color"/>
    <declare-styleable name="WheelPicker">
        <attr name="halfVisibleItemCount"/>
        <attr name="itemTextSize"/>
        <attr name="itemMaximumWidthText"/>
        <attr name="itemTextColor"/>
        <attr name="textGradual"/>
        <attr name="selectedTextColor"/>
        <attr name="selectedTextSize"/>
        <attr name="currentItemPosition"/>
        <attr name="itemWidthSpace"/>
        <attr name="itemHeightSpace"/>
        <attr name="zoomInSelectedItem"/>
        <attr name="wheelCyclic"/>
        <attr name="wheelCurtain"/>
        <attr name="wheelCurtainColor"/>
        <attr name="wheelCurtainBorder" />
        <attr name="wheelCurtainBorderColor"/>
        <attr name="indicatorText"/>
        <attr name="indicatorTextSize"/>
        <attr name="indicatorTextColor"/>
    </declare-styleable>

    <declare-styleable name="YearPicker">
        <attr name="startYear" format="integer"/>
        <attr name="endYear" format="integer"/>
    </declare-styleable>

    <declare-styleable name="MonthPicker">
    </declare-styleable>

    <declare-styleable name="DatePicker">
        <attr name="halfVisibleItemCount"/>
        <attr name="itemTextSize"/>
        <attr name="itemTextColor"/>
        <attr name="textGradual"/>
        <attr name="selectedTextColor"/>
        <attr name="selectedTextSize"/>
        <attr name="itemWidthSpace"/>
        <attr name="itemHeightSpace"/>
        <attr name="zoomInSelectedItem"/>
        <attr name="wheelCyclic"/>
        <attr name="wheelCurtain"/>
        <attr name="wheelCurtainColor"/>
        <attr name="wheelCurtainBorder"/>
        <attr name="wheelCurtainBorderColor"/>
    </declare-styleable>

    <declare-styleable name="HourAndMinutePicker">
        <attr name="halfVisibleItemCount"/>
        <attr name="itemTextSize"/>
        <attr name="itemTextColor"/>
        <attr name="textGradual"/>
        <attr name="selectedTextColor"/>
        <attr name="selectedTextSize"/>
        <attr name="itemWidthSpace"/>
        <attr name="itemHeightSpace"/>
        <attr name="zoomInSelectedItem"/>
        <attr name="wheelCyclic"/>
        <attr name="wheelCurtain"/>
        <attr name="wheelCurtainColor"/>
        <attr name="wheelCurtainBorder"/>
        <attr name="wheelCurtainBorderColor"/>
    </declare-styleable>

    <declare-styleable name="ViewfinderView">
        <attr name="corner_color" format="color"/>
        <attr name="laser_color" format="color"/>
        <attr name="frame_color" format="color" />
        <attr name="mask_color" format="color" />
        <attr name="result_point_color" format="color"/>
        <attr name="result_color" format="color" />
        <attr name="label_text_color" format="color"/>
        <attr name="label_text" format="string"/>
        <attr name="label_text_size" format="float"/>
    </declare-styleable>


    <declare-styleable name="ShimmerRecyclerView">
        <attr name="shimmer_demo_layout" format="reference" />
        <attr name="shimmer_demo_child_count" format="integer|reference" />
        <attr name="shimmer_demo_layout_manager_type" format="enum">
            <enum name="linear_vertical" value="0" />
            <enum name="linear_horizontal" value="1" />
            <enum name="grid" value="2" />
        </attr>
        <attr name="shimmer_demo_grid_child_count" format="integer|reference" />
        <attr name="shimmer_demo_shimmer_color" format="color|reference" />
        <attr name="shimmer_demo_angle" format="integer|reference" />
        <attr name="shimmer_demo_view_holder_item_background" format="color|reference" />
        <attr name="shimmer_demo_duration" format="integer|reference" />
        <attr name="shimmer_demo_mask_width" format="float|reference" />
        <attr name="shimmer_demo_reverse_animation" format="boolean|reference" />
    </declare-styleable>
</resources>